{
    "name": "bengels/schokoladen",
    "type": "project",
    "description": "Schokoladen Web Page",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.3",
        "abrigham/laravel-email-exceptions": "^1.3",
        "anhskohbo/no-captcha": "^3.0",
        "doctrine/dbal": "^2.9",
        "eluceo/ical": "^0.15.0",
        "eternicode/bootstrap-datepicker": "dev-master",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "ixudra/curl": "^6.16",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^4.3",
        "kris/laravel-form-builder": "1.*",
        "kyslik/column-sortable": "^5.8",
        "laravel/framework": "6.x-dev",
        "laravelium/feed": "^3.1",
        "lavary/laravel-menu": "^1.7",
        "misterphilip/maintenance-mode": "~1.3",
        "orchestra/imagine": "^3.8",
        "qafeen/manager": "^0.2.1",
        "spatie/laravel-newsletter": "^4.4",
        "spatie/laravel-permission": "^2.35",
        "symfony/process": "^4.2",
        "tymon/jwt-auth": "1.0.*"
    },

    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/dusk-dashboard": "^1.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "recca0120/terminal": "^1.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
